{
    "name": "ocelot/core",
    "type": "library",
    "description": "Ocelot core package",
    "require": {
        "php": "^7.3",
        "ext-gettext": "*",
        "ext-json": "*",
        "datalinx/gettext-context": "^1.0",
        "doctrine/dbal": "^2.12",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.0",
        "laravel/telescope": "^4.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "symfony/filesystem": "^5.2"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3",
        "orchestra/testbench": "^6.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [],
            "providers": [
                "Ocelot\\Core\\Providers\\CoreServiceProvider"
            ]
        },
        "ocelot": {
            "type": "app"
        }
    },
    "autoload": {
        "psr-4": {
            "Ocelot\\Core\\": "src/",
            "Ocelot\\Core\\Database\\Factories\\": "database/factories/",
            "Ocelot\\Core\\Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Ocelot\\Core\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
