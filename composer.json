{
    "name": "ocelot/core",
    "description": "Ocelot core package",
    "type": "library",
    "require": {
        "php": "^7.3",
        "laravel/framework": "^7.24",
        "laravel/telescope": "^3.5"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Ocelot\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ocelot\\Core\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ocelot\\Core\\Providers\\CoreServiceProvider"
            ]
        }
    }
}
